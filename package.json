{
  "name": "@taylordb",
  "version": "1.0.9",
  "description": "TaylorDb library is a helper library to simplify the use of the TaylorDb database.",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "taylordb": "node packages/cli/dist/index.js",
    "build": "pnpm run compile",
    "prepublishOnly": "pnpm run build",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "format": "gts check",
    "doc:generate": "typedoc",
    "dev": "tsc -b --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webbeetechnologies/taylordb.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/webbeetechnologies/taylordb.git/issues"
  },
  "homepage": "https://github.com/webbeetechnologies/taylordb.git#readme",
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.0.0",
    "eslint-config-prettier": "^10.1.8",
    "gts": "^3.1.1",
    "jest": "^29.7.0",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typedoc": "^0.24.0",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}