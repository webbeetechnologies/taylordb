{
    "name": "@taylordb/query-builder",
    "version": "0.1.4",
    "description": "A type-safe query builder for TaylorDB",
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.ts",
    "private": false,
    "exports": {
      ".": {
        "types": "./dist/src/index.d.ts",
        "import": "./dist/src/index.js",
        "require": "./dist/src/index.js",
        "default": "./dist/src/index.js"
      }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "dev": "tsc -w",
        "example": "tsx sample/sample-1/index.ts"
    },
    "keywords": [
        "query-builder",
        "taylordb"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@taylordb/shared": "workspace:*",
        "json-to-graphql-query": "^2.2.5",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@types/node": "^18.0.0"
    }
}