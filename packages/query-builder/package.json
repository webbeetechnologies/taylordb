{
    "name": "@taylordb/query-builder",
    "version": "0.2.0",
    "description": "A type-safe query builder for TaylorDB",
    "private": false,
    "type": "module",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/esm/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/esm/index.d.ts",
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "clean": "rm -rf dist",
        "build:esm": "tsc -p tsconfig.esm.json",
        "build:cjs": "tsc -p tsconfig.cjs.json",
        "build": "pnpm run clean && pnpm run build:esm && pnpm run build:cjs",
        "dev": "tsc -w",
        "example": "tsx sample/sample-1/index.ts"
    },
    "keywords": [
        "query-builder",
        "taylordb"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@taylordb/shared": "workspace:*",
        "json-to-graphql-query": "^2.2.5",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@types/node": "^18.0.0"
    }
}