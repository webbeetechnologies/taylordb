{
  "name": "@taylordb/cli",
  "version": "0.4.0",
  "description": "CLI for TaylorDB",
  "private": false,
  "main": "./dist/cjs/index.js",
  "bin": {
    "taylordb": "./dist/cjs/index.js"
  },
  "files": [
    "dist",
    "src/templates"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "copy-templates": "cp -r src/templates dist/templates",
    "build": "pnpm run clean && tsc && pnpm run copy-templates",
    "dev": "tsc",
    "start:dev": "ts-node src/index.ts",
    "prepublish": "npm run build"
  },
  "keywords": [
    "cli",
    "taylordb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@inquirer/prompts": "^7.9.0",
    "axios": "^1.7.2",
    "chalk": "^5.3.0",
    "commander": "^8.3.0",
    "conf": "^15.0.2",
    "inquirer": "^12.10.0",
    "json-to-graphql-query": "^2.3.0",
    "lodash": "^4.17.21",
    "ts-morph": "^13.0.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@types/node": "^18.0.0"
  }
}